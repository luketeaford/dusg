{
  "name": "dusg",
  "version": "0.2.0",
  "description": "Deluxe Universal Site Generator",
  "main": "./lib/build.js",
  "bin": "./bin/dusg.js",
  "man": "./man/dusg.1",
  "files": [
    "bin/",
    "lib/",
    "man/"
  ],
  "scripts": {
    "clean": "rm -rf coverage",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "standard --fix",
    "prepublishOnly": "npm run lint && npm test",
    "test": "nyc --reporter=lcov tape test/*.test.js | tap-spec"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luketeaford/dusg.git"
  },
  "keywords": [
    "dusg",
    "deluxe",
    "universal",
    "static",
    "site",
    "generator"
  ],
  "author": "Luke Teaford",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/luketeaford/dusg/issues"
  },
  "homepage": "https://github.com/luketeaford/dusg#readme",
  "devDependencies": {
    "coveralls": "^3.0.9",
    "nyc": "^15.0.0",
    "standard": "^14.3.1",
    "tap-spec": "^5.0.0",
    "tape": "^4.13.0"
  },
  "dependencies": {
    "js-yaml": "^3.13.1",
    "manchego": "^1.1.0",
    "markdown-it": "^10.0.0"
  }
